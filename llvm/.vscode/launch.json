    {
      "name": "gdb_llvm_tblgen",
      "type": "cppdbg",
      "request": "launch",
      "program": "/root/project/llvm-project/builddir/bin/llvm-tblgen",
      "args": [
        "-I/root/project/llvm-project/llvm/lib/Target/IMCE",
        "-I/root/project/llvm-project/llvm/include",
        "/root/project/llvm-project/llvm/lib/Target/IMCE/IMCE.td",
        "--gen-dag-isel"
      ],
      "stopAtEntry": true,
      "cwd": "/root/project/llvm-project/builddir",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "gdb에 자동 서식 지정 사용",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "디스어셈블리 버전을 Intel(으)로 설정",
          "text": "-gdb-set disassembly-flavor intel",
